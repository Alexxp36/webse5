<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="false">

    <servlet>
        <servlet-name>AdministradorController</servlet-name>
        <servlet-class>com.tecsup.demo.controllers.AdministradorController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdministradorController</servlet-name>
        <url-pattern>/validador</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CursoController</servlet-name>
        <servlet-class>com.tecsup.demo.controllers.CursoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CursoController</servlet-name>
        <url-pattern>/cController</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>acceso.jsp</welcome-file>
    </welcome-file-list>
</web-app>